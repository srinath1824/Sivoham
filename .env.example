# ===========================================
# SIVOHAM ENTERPRISE CONFIGURATION
# ===========================================

# Environment
NODE_ENV=development
REACT_APP_VERSION=1.0.0
REACT_APP_BUILD_DATE=2024-01-01

# API Configuration
REACT_APP_API_URL=https://sks-server-production.up.railway.app/api
REACT_APP_API_TIMEOUT=30000
REACT_APP_API_RETRY_ATTEMPTS=3

# Security Configuration
REACT_APP_ENABLE_CSP=true
REACT_APP_SECURE_COOKIES=true
REACT_APP_SESSION_TIMEOUT=3600000

# Monitoring & Analytics
REACT_APP_SENTRY_DSN=your_sentry_dsn_here
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_LOG_LEVEL=info

# Performance Configuration
REACT_APP_ENABLE_SW=true
REACT_APP_CACHE_TIMEOUT=300000
REACT_APP_LAZY_LOADING=true

# Feature Flags
REACT_APP_FEATURE_LOGIN=true
REACT_APP_FEATURE_REGISTRATION=true
REACT_APP_FEATURE_COURSES=true
REACT_APP_FEATURE_EVENTS=true
REACT_APP_FEATURE_ADMIN=true
REACT_APP_FEATURE_PROFILE=true
REACT_APP_FEATURE_OFFLINE=false

# YouTube Data API v3 Configuration
REACT_APP_YOUTUBE_API_KEY=your_youtube_api_key_here
REACT_APP_YOUTUBE_CHANNEL_HANDLE=SivaKundaliniSadhanaChannel
REACT_APP_YOUTUBE_PLAYLIST_ID=PL5n5gvsTFZLxNeqKLWpTPYfdWBU84zKzE

# CDN Configuration
REACT_APP_CDN_URL=https://cdn.sivoham.org
REACT_APP_STATIC_URL=https://static.sivoham.org

# Internationalization
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_LANGUAGES=en,te

# Development Tools
REACT_APP_ENABLE_DEVTOOLS=true
REACT_APP_ENABLE_REDUX_DEVTOOLS=false
REACT_APP_ENABLE_WHY_DID_YOU_RENDER=false